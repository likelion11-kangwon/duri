// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Channel {
  id   String @id
  type String
}

model Meeting {
  id      String   @id
  place   String
  time    String
  slots   Int
  members String[]
}
